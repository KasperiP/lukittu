{
  "compilerOptions": {
    "esModuleInterop": true,
    "noImplicitAny": false,
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./dist",
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es2020",
    "allowSyntheticDefaultImports": true
  }
}
